<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EduStay"
        tools:targetApi="31"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".Comman.Help_center_Activity"
            android:exported="false" />
        <activity
            android:name=".Comman.Theme_Activity"
            android:exported="false" />
        <activity
            android:name=".Comman.About_Us_Activity"
            android:exported="false" />
        <activity
            android:name=".Comman.Password_Security_Activity"
            android:exported="false" />
        <activity
            android:name=".Comman.Manager_Profile_Activity"
            android:exported="false" />
        <activity
            android:name=".Library_module.Library_Home_Activity"
            android:exported="false" />
        <activity
            android:name=".Hostel_module.Hostel_Home_Activity"
            android:exported="false" />
        <activity
            android:name=".ModuleActivity"
            android:exported="false" />
        <activity
            android:name=".RegistrationActivity"
            android:exported="false" />
        <activity
            android:name=".LoginActivity"
            android:exported="true" />
        <activity
            android:name=".SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>